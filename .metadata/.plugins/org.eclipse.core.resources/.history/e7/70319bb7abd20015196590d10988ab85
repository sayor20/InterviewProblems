import java.util.ArrayList;
import java.util.HashSet;

public class Test110 {

	public static void main(String[] args) {
		HashSet<ArrayList<Integer>> out = new HashSet<ArrayList<Integer>>();
		ArrayList<Integer> temp = new ArrayList<Integer>();
		comb(new int[]{2,3,6,7} ,7, 0, 0, temp, out);	
	}

	private static void comb(int[] inp, int t, int tot, int tempNum,ArrayList<Integer> tempList, HashSet<ArrayList<Integer>> out) {
		if(tot==t){
			if(tempList!=null)
				out.add(tempList);
		}
		for(int i=tempNum; i<inp.length; i++){	
			if(inp[i]>t)
				return;
			tempList.add(inp[i]);
			comb(inp, t, tot+inp[i] ,i, tempList, out);
		}
		System.out.println(out);
	}

}
