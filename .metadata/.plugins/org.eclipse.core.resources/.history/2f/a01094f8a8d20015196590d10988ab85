import java.util.ArrayList;
import java.util.HashSet;

public class Test110 {

	public static void main(String[] args) {
		HashSet<ArrayList<Integer>> out = new HashSet<ArrayList<Integer>>();
		ArrayList<Integer> temp = new ArrayList<Integer>();
		comb(new int[]{2,3,6,7} ,7, 0, 2, temp, out);	
	}

	private static void comb(int[] inp, int t, int tot, int tempNum,ArrayList<Integer> tempList, HashSet<ArrayList<Integer>> out) {
		tot += tempNum;
		tempList.add(tempNum);
		if(tot==t){
			if(tempList!=null)
				out.add(tempList);
		}else if(tot<t){
			for(int i=0;inp[i]<t;i++){	
				comb(inp, t, tot, inp[i+1] ,tempList, out);
			}
		}else{
			tempList = null;
			tempNum = 0;
		}	
		System.out.println(out);
	}

}
